package univer_lesson.binaryHeapTree;

public class Main {
    public static void main(String[] args) {
        BinaryHeapTree heap = new BinaryHeapTree(10);
        for (int i = 0; i <= 10; i++)
            heap.insert(i);

        System.out.println(heap);

        heap.delete(3);
        System.out.println(heap);

        heap.delete(7);
        System.out.println(heap);
    }
}
